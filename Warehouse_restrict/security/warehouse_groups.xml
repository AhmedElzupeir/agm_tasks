            <field name="groups" eval="[(4, ref('stock.group_stock_user')), (4, ref('stock.group_stock_manager'))]"  />
        </record>
        <record id="picking_rules_member" model="ir.rule">
            <field name="name">picking rule</field>
            <field name="model_id" search="[('model','=','stock.picking')]" model="ir.model"/>
            <field name="domain_force">[('location_id' , 'like' , user.warehose_ids.code)]</field>
            <field name="groups" eval="[(4, ref('stock.group_stock_user')), (4, ref('stock.group_stock_manager'))]"  />
        </record>
    </data>
</odoo>