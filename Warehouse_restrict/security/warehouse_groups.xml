<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
        <record id="warehouse_rules_member" model="ir.rule">
            <field name="name">Warehouse rule</field>
            <field name="model_id" search="[('model','=','stock.warehouse')]" model="ir.model"/>
            <field name="domain_force">[('id', 'in', user.warehose_ids.ids)]</field>
            <field name="groups" eval="[(4, ref('stock.group_stock_user')), (4, ref('stock.group_stock_manager'))]"  />
        </record>
        <record id="location_rules_member" model="ir.rule">
            <field name="name">location rule</field>
            <field name="model_id" search="[('model','=','stock.location')]" model="ir.model"/>
            <field name="domain_force">[('location_id' , 'like' , user.warehose_ids.code)]</field>
            <field name="groups" eval="[(4, ref('stock.group_stock_user')), (4, ref('stock.group_stock_manager'))]"  />
        </record>
            <record id="scrap_rules_member" model="ir.rule">
